  },
  "keywords": [
    "remark",
    "remark-plugin"
  ],
  "author": "sandypockets",
  "license": "MIT",
  "dependencies": {
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "rehype-stringify": "^10.0.0",
    "remark-rehype": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-loader": "^9.1.3",
    "prettier": "3.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
