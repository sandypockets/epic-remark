{
  "name": "epic-remark",
  "version": "0.1.0",
  "description": "Epic Remark is an all-in-one markdown to HTML processor built on top of remark",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "prep": "yarn prettier . --write && yarn build",
